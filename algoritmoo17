package main

import "fmt"

func main() {
	var num float64
	fmt.Println("digite seu salário:")
	fmt.Scan(&num)
	if num < 1000 {
		novo_salario := (num * 0.1) + num
		fmt.Printf("seu novo salário é de: %.2f", novo_salario)
	} else {
		novo_salario := (num * 0.05) + num
		fmt.Printf("seu novo salário é de: %.2f", novo_salario)
	}
}
