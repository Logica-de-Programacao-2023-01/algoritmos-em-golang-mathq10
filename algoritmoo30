package main

import "fmt"

func main() {
	var num, maior_numero int
	fmt.Println("digite alguns números(COLOQUE 0 CASO QUEIRA PARAR):")
	for {
		fmt.Scan(&num)

		if num == 0 {
			break
		}
		if num > maior_numero {
			maior_numero = num
		}

	}
	fmt.Printf("Ó maior número é o:\n %d", maior_numero)
}
